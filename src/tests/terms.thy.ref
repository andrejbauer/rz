[Processing terms.thy]
module type Terms = 
sig
  type s
  (**  Assertion per_s =  PER(=s=)
  *)
   
  val a : s
  (**  Assertion a_total =  a : ||s||
  *)
   
  val b : s
  (**  Assertion b_total =  b : ||s||
  *)
   
  val c : s
  (**  Assertion c_total =  c : ||s||
  *)
   
  type p
  (**  Assertion predicate_p =  PREDICATE(p, p)
  *)
   
  val emptyTuple : unit
  (**  Assertion emptyTuple_def =  true
  *)
   
  val var : s
  (**  Assertion var_def =  var =s= a
  *)
   
  val tuple : s * s * s
  (**  Assertion tuple_def =  pi0 tuple =s= a /\ pi1 tuple =s= b /\ 
         pi2 tuple =s= c
  *)
   
  val proj : s
  (**  Assertion proj_def =  proj =s= b
  *)
   
  val g : (s -> s) -> s -> s
  (**  Assertion g_def = 
         forall x:s -> s, y:s -> s, 
           (forall x':s, y':s,  x' =s= y' -> x x' =s= y y') ->
           forall x':s, y':s,  x' =s= y' -> g x x' =s= y y'
  *)
   
  val application : s -> s
  (**  Assertion application_def = 
         forall x:s, y:s,  x =s= y -> application x =s= g (fun x' : s => a) y
  *)
   
  val description : s * p
  (**  Assertion description_def = 
         pi1 description |= p (pi0 description) /\ 
         pi0 description =s= (assure x:s, y:p.
                                  (x : ||s|| /\ y |= p x /\ 
                                   (forall (x':||s||), 
                                      forall z:p,  z |= p x' -> x =s= x'))
                                in x)
  *)
   
  val cases : s
  (**  Assertion cases_def =  cases =s= b
  *)
   
  val r : s
  (**  Assertion r_total =  r : ||s||
  *)
   
  val rzquot : s
  (**  Assertion rzquot_def =  rzquot =s= r
  *)
   
  val rzchoose : s
  (**  Assertion rzchoose_def =  rzchoose =s= a
  *)
   
  
  (**  Assertion predicate_eq =  PREDICATE(eq)
  *)
   
  val quot : s
  (**  Assertion quot_def =  eq quot a
  *)
   
  val choose_term : s
  (**  Assertion choose_term_def =  eq choose_term quot
  *)
   
  val let_term : s * s
  (**  Assertion let_term_def =  pi0 let_term =s= a /\ pi1 let_term =s= a
  *)
   
  val subin : s * p
  (**  Assertion subin_def =  pi1 subin |= p (pi0 subin) /\ 
         (assure x:p. (x |= p a) in x) |= p a /\ pi0 subin =s= a
  *)
   
  (** fishy *)
   
  val d : s * p
  (**  Assertion d_total =  pi0 d : ||s|| /\ pi1 d |= p (pi0 d)
  *)
   
  val subout : s
  (**  Assertion subout_def =  subout =s= pi0 d
  *)
end

